  "author": "Dragoș Străinu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strdr4605/md-jump2/issues"
  },
  "homepage": "https://github.com/strdr4605/md-jump2#readme",
  "devDependencies": {
    "@types/node": "^13.9.5",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.0",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
